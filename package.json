{
  "name": "fetchq-documentation",
  "version": "0.0.2",
  "description": "Fetchq Gitbook Documentation Project",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "./node_modules/.bin/gitbook serve src docs",
    "serve": "yarn start",
    "build": "./node_modules/.bin/gitbook build src docs",
    "deploy": "yarn build && git add . && git commit -m \"publish\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fetchq/gitbook.git"
  },
  "author": "Marco Pegoraro @marcopeg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fetchq/gitbook/issues"
  },
  "homepage": "https://fetchq.com",
  "dependencies": {
    "gitbook-cli": "^2.3.2"
  }
}
